{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    ["module-resolver", {
      "root": "./",
      "alias": {
        "assets": "./assets",
        "components": "./components",
        "Fire": "./Fire",
        "lib": "./lib",
        "stores": "./stores",
        "styles": "./styles",
        "views": "./views"
      }
    }]
  ]
}
